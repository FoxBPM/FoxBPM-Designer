<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            class="org.foxbpm.bpmn.designer.ui.wizard.FoxFlowNewWizard"
            id="org.foxbpm.bpmn.designer.ui.newbpmwizard"
            name="%wizard.name">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.foxbpm.bpmn.designer.ui.editors.FoxBPMEditor"
            default="true"
            icon="icons/fox_16.ico"
            id="org.foxbpm.bpmn.designer.ui.editor"
            name="%editor.name">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.foxbpm.bpmn.designer.ui.propertyContributor"
            labelProvider="org.foxbpm.bpmn.designer.ui.propertytab.FoxBPMPropertyLabelProvider"
            tabDescriptorProvider="org.foxbpm.bpmn.designer.ui.propertytab.FoxBPMTabDescriptorProvider">
         <propertyCategory
               category="BPMN"></propertyCategory>
      </propertyContributor>
   </extension>
 <extension
       point="org.eclipse.ui.commands">
    <command
          id="org.foxbpm.bpmn.designer.ui.command"
          name="%command.name">
    </command>
 </extension>
 <extension
       point="org.eclipse.ui.handlers">
    <handler
          commandId="org.foxbpm.bpmn.designer.ui.command">
    </handler>
 </extension>
 <extension
       point="org.eclipse.ui.menus">
    <menuContribution
          allPopups="false"
          locationURI="menu:org.eclipse.ui.main.menu?after=additions">
       <menu
             label="FoxBPM">
          <command
                commandId="org.foxbpm.bpmn.designer.ui.command"
                style="push">
          </command>
       </menu>
    </menuContribution>
 </extension>
 <extension
       point="org.eclipse.core.expressions.propertyTesters">
    <propertyTester
          class="org.foxbpm.bpmn.designer.ui.propertytester.FoxBPMMenuPropertyTester"
          id="org.foxbpm.bpmn.designer.ui.propertyTester"
          namespace="org.foxbpm.bpmn.designer.ui"
          properties="targetRuntimeId"
          type="java.lang.Object">
    </propertyTester>
 </extension>
 <extension
       point="org.eclipse.ui.perspectives">
    <perspective
          class="org.foxbpm.bpmn.designer.ui.perspectives.FoxBPMPerspective"
          icon="icons/fox_16.ico"
          id="org.foxbpm.bpmn.designer.ui.foxbpmperspective"
          name="%persperctive.name">
    </perspective>
 </extension>
 <extension
       point="org.eclipse.ui.views">
    <view
          category="org.foxbpm.bpmn.designer.ui.foxbpmcategory"
          class="org.foxbpm.bpmn.designer.ui.views.FoxBPMNavigatorView"
          icon="icons/fox_16.ico"
          id="org.foxbpm.bpmn.designer.ui.foxbpmview"
          name="%navigatorview.name"
          restorable="true">
    </view>
    <category
          id="org.foxbpm.bpmn.designer.ui.foxbpmcategory"
          name="%views.category">
    </category>
 </extension>
 <extension
       point="org.eclipse.ui.navigator.viewer">
    <viewer
          viewerId="org.foxbpm.bpmn.designer.ui.foxbpmview">
       <popupMenu
             allowsPlatformContributions="true"
             id="org.foxbpm.bpmn.designer.ui.popmenu">
          <insertionPoint
                name="group.new">
          </insertionPoint>
       </popupMenu>
    </viewer>
    <viewerContentBinding
          viewerId="org.foxbpm.bpmn.designer.ui.foxbpmview">
       <includes>
          <contentExtension
                pattern="org.foxbpm.bpmn.designer.ui.navigatorContent">
          </contentExtension>
          <contentExtension
                pattern="org.eclipse.ui.navigator.resources.filters.*">
          </contentExtension>
          <contentExtension
                isRoot="true"
                pattern="org.e clipse.ui.navigator.resourceContent">
          </contentExtension></includes>
    </viewerContentBinding>
    <viewerActionBinding
          viewerId="org.foxbpm.bpmn.designer.ui.foxbpmview">
       <includes>
          <actionExtension
                pattern="org.eclipse.ui.navigator.resources.*">
          </actionExtension>
          <actionExtension
                pattern="org.foxbpm.openaction.provider.*">
          </actionExtension>
       </includes>
    </viewerActionBinding>
    <dragAssistant
          class="org.foxbpm.bpmn.designer.ui.views.dragassistant.FoxBPMDragAdapterAssistant"
          viewerId="org.foxbpm.bpmn.designer.ui.dragAssistant1">
    </dragAssistant>
 </extension>
 <extension
       point="org.eclipse.ui.navigator.navigatorContent">
    <navigatorContent
          contentProvider="org.foxbpm.bpmn.designer.ui.views.FoxBPMNaviViewContentProvider"
          id="org.foxbpm.bpmn.designer.ui.navigatorContent"
          labelProvider="org.foxbpm.bpmn.designer.ui.views.FoxBPMNaviViewLabelProvider"
          name="%navigatorcontent.name">
       <triggerPoints></triggerPoints>
    </navigatorContent>
    <commonWizard
          type="import"
          wizardId="org.eclipse.ui.wizards.new.folder">
       <enablement>
          <or>
             <adapt
                   type="org.eclipse.core.resources.IFile">
             </adapt>
             <adapt
                   type="org.eclipse.core.resources.IFolder">
             </adapt>
             <adapt
                   type="org.eclipse.core.resources.IProject">
             </adapt>
             <adapt
                   type="org.eclipse.core.resources.IWorkspaceRoot">
             </adapt>
          </or></enablement>
    </commonWizard>
    <actionProvider
          class="org.foxbpm.bpmn.designer.ui.views.action.FoxBPMNaviViewActionProvider"
          id="org.foxbpm.openaction.provider">
       <enablement>
          <or>
             <adapt
                   type="org.eclipse.core.resources.IFile">
             </adapt>
          </or>
       </enablement>
    </actionProvider>
 </extension>
 <extension
       point="org.eclipse.ui.ide.projectNatureImages">
    <image
          icon="icons/fox_16.ico"
          id="org.foxbpm.bpmn.designer.ui.image"
          natureId="org.foxbpm.bpmn.designer.ui.foxbpmproject">
    </image>
 </extension>
	<extension point="org.foxbpm.runtime">
		<runtime class="org.eclipse.bpmn2.modeler.ui.DefaultBpmn2RuntimeExtension"
			description="%runtime.description" id="org.eclipse.bpmn2.modeler.runtime.none"
			name="%runtime.name">
		</runtime>

		<model runtimeId="org.eclipse.bpmn2.modeler.runtime.none"
			uri="http://www.omg.org/spec/BPMN/20100524/MODEL-XMI"
			resourceFactory="org.eclipse.bpmn2.modeler.core.model.Bpmn2ModelerResourceFactoryImpl">
		</model>

		<!-- ======================================== -->
		<!-- ========= Default EndPoint Ext ========= -->
		<!-- ======================================== -->
		
		
		<!-- ======================================== -->
		<!-- ============  Property Tabs ============ -->
		<!-- ======================================== -->

		<toolPalette
			id="org.bpmn2.modeler.toolpalette.process.default"
			runtimeId="org.eclipse.bpmn2.modeler.runtime.none"
			type="Process" profile="foxbpm">
			
			<category id="org.bpmn2.modeler.toolpalette.default.categories"/>
			
			
		</toolPalette>
  <modelEnablement
        profile="FOXBPM"
        runtimeId="org.foxbpm.bpmn.designer.ui.modelEnablement1"
        type="Process">
     <enable
           object="StartEvent">
     </enable>
     <enable
           object="UserTask">
     </enable>
     <enable
           object="EndEvent">
     </enable>
  </modelEnablement>
  <propertyTab
        class="org.foxbpm.bpmn.designer.ui.propertytab.section.event.StartEventPropertySection"
        id="org.foxbpm.bpmn.designer.ui.propertyTab.startevent"
        label="通用">
  </propertyTab>

		<!-- ======================================== -->
		<!-- ===  Model Enablements and Profiles ==== -->
		<!-- ======================================== -->






	</extension>
</plugin>
